    <contexts>
      <context name="Normal" attribute="Normal Text" lineEndContext="#stay">
        <!-- Include directive -->
        <RegExpr attribute="Preprocessor" context="#stay" String="^\s*#\s*include\b.*$"/>

        <!-- Block declarations -->
        <RegExpr attribute="Keyword" context="#stay" String="\b(functions|data|transformed\s+data|parameters|transformed\s+parameters|model|generated\s+quantities)\b"/>

        <!-- Comments -->
        <Detect2Chars attribute="Comment" context="BlockComment" char="/" char1="*"/>
        <Detect2Chars attribute="Comment" context="LineComment" char="/" char1="/"/>
        <DetectChar attribute="Comment" context="LineComment" char="#"/>

        <!-- Strings -->
        <DetectChar attribute="String" context="String" char="&quot;"/>

        <!-- Numbers -->
        <RegExpr attribute="Float" context="#stay" String="\b[0-9]+\.[0-9]*([eE][+-]?[0-9]+)?\b"/>
        <RegExpr attribute="Float" context="#stay" String="\b\.[0-9]+([eE][+-]?[0-9]+)?\b"/>
        <RegExpr attribute="Float" context="#stay" String="\b[0-9]+[eE][+-]?[0-9]+\b"/>
        <RegExpr attribute="DecVal" context="#stay" String="\b[0-9]+\b"/>

        <!-- Jacobian functions (special highlight) -->
        <keyword attribute="BuiltIn" context="#stay" String="jacobian_functions"/>

        <!-- User-defined jacobian functions -->
        <RegExpr attribute="BuiltIn" context="#stay" String="\b[A-Za-z][A-Za-z0-9_]*_jacobian\b"/>

        <!-- Types -->
        <keyword attribute="DataType" context="#stay" String="types"/>

        <!-- Control flow -->
        <keyword attribute="ControlFlow" context="#stay" String="control"/>

        <!-- Special keywords -->
        <keyword attribute="Keyword" context="#stay" String="special_keywords"/>

        <!-- Distributions after ~ -->
        <RegExpr attribute="Operator" context="Distribution" String="~\s*"/>

        <!-- Built-in functions -->
        <keyword attribute="Function" context="#stay" String="functions"/>
        <keyword attribute="Function" context="#stay" String="distributions"/>

        <!-- Operators -->
        <RegExpr attribute="Operator" context="#stay" String="\|\||&amp;&amp;|==|!=|&lt;=?|&gt;=?"/>
        <RegExpr attribute="Operator" context="#stay" String="[+\-]=|\.[*/]=|="/>
        <RegExpr attribute="Operator" context="#stay" String="[+\-]|\.\*|\./|[*/%\\^']"/>
        <DetectChar attribute="Operator" context="#stay" char="!"/>
        <DetectChar attribute="Operator" context="#stay" char="?"/>
        <DetectChar attribute="Operator" context="#stay" char=":"/>
      </context>

      <context name="Distribution" attribute="Function" lineEndContext="#pop">
        <keyword attribute="Function" context="#pop" String="distributions"/>
        <RegExpr attribute="Normal Text" context="#pop" String="\S"/>
      </context>

      <context name="BlockComment" attribute="Comment" lineEndContext="#stay">
        <Detect2Chars attribute="Comment" context="#pop" char="*" char1="/"/>
      </context>

      <context name="LineComment" attribute="Comment" lineEndContext="#pop">
      </context>

      <context name="String" attribute="String" lineEndContext="#pop">
        <DetectChar attribute="String" context="#pop" char="&quot;"/>
      </context>
    </contexts>

